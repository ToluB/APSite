<h1>Listing exams</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Nextadm</th>
    <th>Lastadm</th>
    <th>Passrt</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @exams.each do |exam| %>
  <tr>
    <td><%= exam.name %></td>
    <td><%= exam.nextadm %></td>
    <td><%= exam.lastadm %></td>
    <td><%= exam.passrt %></td>
    <td><%= exam.url %></td>
    <td><%= link_to 'Show', exam %></td>
    <td><%= link_to 'Edit', edit_exam_path(exam) %></td>
    <td><%= link_to 'Destroy', exam, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New exam', new_exam_path %>
