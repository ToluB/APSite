<h1>Listing colleges</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Region</th>
    <th>State</th>
    <th>Usnrank</th>
    <th>Satmed</th>
    <th>Gpamed</th>
    <th>Acceptrate</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @colleges.each do |college| %>
  <tr>
    <td><%= college.name %></td>
    <td><%= college.region %></td>
    <td><%= college.state %></td>
    <td><%= college.usnrank %></td>
    <td><%= college.satmed %></td>
    <td><%= college.gpamed %></td>
    <td><%= college.acceptrate %></td>
    <td><%= college.url %></td>
    <td><%= link_to 'Show', college %></td>
    <td><%= link_to 'Edit', edit_college_path(college) %></td>
    <td><%= link_to 'Destroy', college, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New college', new_college_path %>
